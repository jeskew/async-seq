{
  "name": "@jsq/async-seq",
  "version": "0.3.0",
  "description": "Lazy sequence operators for JavaScript backed by async iterators.",
  "main": "lib/index.js",
  "module": "lib/index.mjs",
  "types": "lib/index.d.ts",
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "build": "tsc && rollup -c ./rollup.config.js",
    "coverage": "nyc --reporter=lcov --reporter=html npm test && nyc report",
    "docs": "typedoc src",
    "prepublishOnly": "npm run build",
    "pretest": "tsc -p tsconfig.test.json",
    "release": "standard-version",
    "test": "tape lib/*.spec.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jeskew/async-seq.git"
  },
  "keywords": [
    "asynciterator",
    "seq"
  ],
  "author": "Jonathan Eskew <jonathan@jeskew.net>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/jeskew/async-seq/issues"
  },
  "homepage": "https://github.com/jeskew/async-seq#readme",
  "devDependencies": {
    "@types/tape": "^4.2.31",
    "nyc": "^11.3.0",
    "rollup": "^0.56.4",
    "rollup-plugin-typescript": "^0.8.1",
    "standard-version": "^4.2.0",
    "tape": "^4.9.0",
    "typedoc": "^0.11.0",
    "typescript": "^2.7"
  },
  "dependencies": {
    "tslib": "^1.8.1"
  },
  "nyc": {
    "exclude": [
      "**/*.spec.js",
      "**/*.fixture.js"
    ]
  }
}
