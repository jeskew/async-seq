{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": [
      "es5",
      "es2015.collection",
      "es2015.promise",
      "esnext.asynciterable"
    ],
    "declaration": true,
    "sourceMap": true,
    "outDir": "./lib",
    "rootDir": "./src",
    "downlevelIteration": true,
    "importHelpers": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true
  },
  "typedocOptions": {
    "mode": "file",
    "out": "docs",
    "name": "@jsq/seq",
    "exclude": "**/*.+(spec|fixture).ts",
    "excludeNotExported": true,
    "excludePrivate": true
  }
}
